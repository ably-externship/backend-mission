### 쇼핑몰 생성 처리
POST http://localhost:8000/shop
Content-Type: application/json

{
  "shop_name": "슬로우앤드",
  "img_url": "data:image/jpeg;base64,",
  "category_name": "여성의류"
}


### 아이템 생성 처리
POST  http://localhost:8000/shop/1/items
Content-Type: application/json

{
    "product_name":"패딩",
    "description":"따뜻",
    "price":900008,
    "stock":94,
    "option_group" : [{"optionGroup":"사이즈",
                        "optionItem":[{
                            "optionItem":"55",
                            "addPrice":0
                        },{
                            "optionItem":"66",
                            "addPrice":500
                        }]}
                     ,{"optionGroup":"색상",
                        "optionItem":[{
                            "optionItem":"black",
                              "addPrice":0
                        },
                        {
                            "optionItem":"red",
                            "addPrice":700
                        }]}]
}

### 아이템 생성 처리
POST  http://localhost:8000/shop/2/items
Content-Type: application/json

{
    "product_name":"니트",
    "description":"추울때",
    "price":100000,
    "stock":150,
    "option_group" : [{"optionGroup":"사이즈",
                        "optionItem":[{
                            "optionItem":"55",
                            "addPrice":0
                        },{
                            "optionItem":"88",
                            "addPrice":1500
                        }]}
                     ,{"optionGroup":"색상",
                        "optionItem":[{
                            "optionItem":"beige",
                              "addPrice":0
                        },
                        {
                            "optionItem":"brown",
                            "addPrice":1000
                        }]}]
}
